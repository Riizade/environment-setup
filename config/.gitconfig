[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cd) %C(bold blue)<%an>%Creset' --abbrev-commit
    undocommit = reset HEAD~

[push]
    default = current
    autoSetupRemote = true

[pull]
	ff = only
	default = current

[fetch]
	prune = true

[protocol]
	version = 2